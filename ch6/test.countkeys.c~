#include <stdio.h> 
#include <ctype.h>
#include <string.h>

/* count occurrences of C keywords (in C program) */

#define MAXWORD 100

/* struct of keyword structs - 
NOTE: not exhaustive .. did not feel like typing all..
but sorted (important)
*/
struct key {
  char *word;
  int count;
} keytab[] = {
  "auto", 0,
  "break", 0,
  "case", 0,
  "char", 0,
  "const", 0,
  "continue", 0,
  "default", 0,
  "int", 0,
  "unsigned", 0,
  "void", 0,
  "volatile", 0,
  "while", 0
};

