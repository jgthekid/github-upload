
  /* int */
  i = 0;
  int inext = 0, ilast = 0;
  /* going up */
  inext = ilast + 1;
  while (inext > ilast) {
    ilast = inext;
    ++inext;
    ++i;
  }
  printf("biggest int: %d\n", ilast);

  /* going down */
  inext = 0; ilast = 0;
  inext = ilast - 1;
  i = 0;
  while (inext < ilast) {
    ilast = inext;
    --inext;
    ++i;
  }
  printf("smallest int: %d\n", ilast);
